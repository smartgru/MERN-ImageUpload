{
  "name": "image-api",
  "version": "1.0.0",
  "private": true,
  "description": "Back-end for Imange management",
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Liu Jang",
  "license": "MIT",
  "scripts": {
    "clean:hard": "rm -Rf ./build/* && rm -Rf ./logs/* && rm -Rf ./node_modules/",
    "debug": "nodemon --inspect src/app.js --ignore 'build/' --ignore '.git/' --ignore 'public/' --exec babel-node",
    "start": "nodemon src/app.js --ignore 'build/' --ignore 'public/' --exec babel-node",
    "foreverstart": "forever start --uid image-api --append build/app.js",
    "build": "babel src -d build",
    "task": "nodemon src/tasks/index.js --ignore 'uploads/' --exec babel-node",
    "test": "NODE_ENV=test mocha --compilers js:@babel/register",
    "migrate": "babel-node src/migrate/index.js",
    "pretty": "prettier --write \"src/**/*.js\"",
    "lint": "eslint --fix src",
    "precommit": "lint-staged && npm test"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-promise": "^3.5.0",
    "factory-girl": "^5.0.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^3.4.2"
  },
  "dependencies": {
    "autolinker": "^1.4.4",
    "base64url": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-prettify": "^0.0.8",
    "express-winston": "^2.3.0",
    "faker": "^4.1.0",
    "forever": "^0.15.3",
    "jsonexport": "^2.0.10",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.13",
    "method-override": "^2.3.10",
    "moment": "^2.18.1",
    "mongo-sanitize": "^1.0.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.7.5",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-deep-populate": "^3.1.1",
    "mongoose-merge-plugin": "0.0.5",
    "multer": "^1.4.2",
    "node-code-generator": "^1.0.3",
    "node-schedule": "^1.2.5",
    "null-prune": "^1.0.3",
    "numeral": "^2.0.6",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.5",
    "request-promise": "^4.2.4",
    "sendgrid": "^5.2.3",
    "sharp": "^0.25.4",
    "string": "^3.3.3",
    "validator": "^8.2.0",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "engines": {
    "npm": "^6.0.0",
    "node": "^8.0.0"
  }
}
